        <mxCell id="ZO9Wsin6s2xFnNDmiBNk-6" value="&lt;span style=&quot;font-size: 15px;&quot;&gt;&lt;font face=&quot;Helvetica&quot;&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;巴西&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;RENOVATE&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;试验的&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;510&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;名患者和美国&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;MIMIC-IV&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;与&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;eICU&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;数据库的&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;57&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;名患者共同组成的&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;567&lt;/font&gt;&lt;font style=&quot;font-size: 15px;&quot;&gt;名患者作为外部验证集&lt;/font&gt;&lt;/font&gt;&lt;/span&gt;" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
